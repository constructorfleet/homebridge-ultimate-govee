    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "homebridge": "^1.8.2",
    "jest": "^27.5.1",
    "nodemon": "^3.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
