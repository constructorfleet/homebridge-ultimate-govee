{
  "private": false,
  "displayName": "Govee Ultimate",
  "name": "homebridge-ultimate-govee",
  "author": {
    "name": "Teagan Glenn",
    "email": "that@teagantotally.rocks"
  },
  "version": "2.93.1",
  "type": "commonjs",
  "description": "The Ultimate Govee HomeBridge plugin.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/constructorfleet/homebridge-ultimate-govee.git"
  },
  "bugs": {
    "url": "https://github.com/constructorfleet/homebridge-ultimate-govee/issues"
  },
  "engines": {
    "node": ">=20.7.0",
    "homebridge": ">=1.7.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "test": "jest",
    "start": "npm run build && npm link && ./node_modules/homebridge/bin/homebridge -I -D | tee /home/node/.homebridge/homebridge.log",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "keywords": [
    "homebridge-plugin",
    "govee"
  ],
  "dependencies": {
    "@constructorfleet/ultimate-govee": "^1.0.0-rc1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "async-await-mutex-lock": "^1.0.11",
    "class-transformer": "^0.5.1",
    "es6-shim": "^0.35.8",
    "pure-color": "^1.3.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "simple-diff": "^1.7.2",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.11.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "homebridge": "^1.7.0",
    "jest": "^27.5.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
