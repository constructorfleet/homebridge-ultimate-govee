    "@nestjs/core": "^10.3.8",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "async-await-mutex-lock": "^1.0.11",
    "class-transformer": "^0.5.1",
    "es6-shim": "^0.35.8",
    "homebridge-config-ui-x": "^4.56.2",
